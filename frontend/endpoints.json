{
    "studentRegister": {
        "url": "/studentRegister",
        "params": ["regCode", "username", "password"],
        "requestType": "POST"
    },
    "instructorRegister": {
        "url": "/instructorRegister",
        "params": ["regCode", "username", "password"],
        "requestType": "POST"
    },
    "login": {
        "url": "/login",
        "params": ["username", "password"],
        "requestType": "GET"
    },
    "sendEmail": {
        "url": "/sendEmail",
        "params": ["accoundId", "emailSubject", "emailBody", "isBodyHtml"],
        "requestType": "GET",
        "notes": "please use 1 or 0 for isBodyHtml"
    },
    "createReview": {
        "url": "/createReview",
        "params": ["accoundId", "courseId", "title", "notes"],
        "requestType": "POST"
    },
    "deleteReview": {
        "url": "/deleteReview",
        "params": ["reviewId"],
        "requestType": "POST"
    },
    "getReviews": {
        "url": "/getReviews",
        "params": ["accountId", "courseId"],
        "requestType": "GET",
        "notes": "student will get only reviews they created, instructor will get all reviews of a course"
    }
}