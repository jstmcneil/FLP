{
    "studentRegister": {
        "url": "/studentRegister",
        "params": ["regCode", "username", "password"],
        "requestType": "POST"
    },
    "instructorRegister": {
        "url": "/instructorRegister",
        "params": ["regCode", "username", "password"],
        "requestType": "POST"
    },
    "login": {
        "url": "/login",
        "params": ["username", "password"],
        "requestType": "POST"
    },
    "getAccount": {
        "url": "/getAccount",
        "params": [],
        "requestType": "GET"
    },
    "sendEmail": {
        "url": "/sendEmail",
        "params": ["accoundId", "emailSubject", "emailBody", "isBodyHtml"],
        "requestType": "POST"
    },
    "createReview": {
        "url": "/createReview",
        "params": ["accoundId", "courseId", "review"],
        "requestType": "POST"
    },
    "getReviews": {
        "url": "/getReviews",
        "params": ["accountId", "courseId"],
        "requestType": "GET",
        "notes": "student will get only reviews they created, instructor will get all reviews of a course"
    },
    "setCurriculum": {
        "url": "/setCurriculum",
        "params": ["regCode", "courses"],
        "requestType": "POST",
        "notes": "courses: [courseId]"
    },
    "getCurriculum": {
        "url": "/getCurriculum",
        "params": ["regCode"],
        "requestType": "GET"
    },
    "submitQuiz": {
        "url": "/submitQuiz",
        "params": ["accountId", "courseId", "answers", "completedEmailQuestion"],
        "requestType": "POST",
        "notes": "answers: [{questionId: Number, answerIndex: Number}]"
    },
    "getCourses": {
        "url": "/getCourses",
        "params": ["regCode"],
        "requestType": "GET"
    },
    "getAllGrades": {
        "url": "/getAllGrades",
        "params": ["accountId", "regCode"],
        "requestType": "GET"
    },
    "getAllCourses": {
        "url": "/getAllCourses",
        "params": [],
        "requestType": "GET"
    }
}